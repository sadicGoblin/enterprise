{
  "hosting": {
    "public": "dist/enterprise-app/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ws/**",
        "destination": "https://inarco-ssoma.favric.cl/ws/**"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}